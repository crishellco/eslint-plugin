{
  "name": "@crishellco/eslint-plugin",
  "version": "0.0.0-semantically-released",
  "author": "Chris Mitchell (@crishellco)",
  "description": "Opinionated ESLint Plugin for Vue.js projects.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crishellco/eslint-plugin.git"
  },
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "bugs": {
    "url": "https://github.com/crishellco/eslint-plugin/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:js": "eslint .",
    "lint:lockfile": "lockfile-lint",
    "test:unit": "jest",
    "lint:peer": "npm ls >/dev/null"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@form8ion/commitlint-config": "^1.0.29",
    "cz-conventional-changelog": "3.3.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-eslint-plugin": "4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "27.4.7",
    "lockfile-lint": "4.6.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "semantic-release": "^19.0.2"
  },
  "dependencies": {
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "8.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
